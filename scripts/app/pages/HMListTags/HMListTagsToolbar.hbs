<div id="HMListTagsToolbar" class="row">
	<div class="span5" style="background-color: #eee">
		<div class="row">
			<div class="span5 text-center"><span><i class="icon-eye-open"></i> Categories</span></div>
		</div>
		<div class="row">
			<center>
				<div class="span5 btn-group btn-toolbar-views">
					{{#linkTo 'HMListTags.bigIcons' classNames="btn"}}<i class="icon-th-large"></i> Big Icons{{/linkTo}}
					{{#linkTo 'HMListTags.windRose' classNames="btn"}}<i class="icon-compass"></i> Wind Rose{{/linkTo}}
					{{#linkTo 'HMListTags.users' classNames="btn"}}<i class="icon-group"></i> Users{{/linkTo}}
					{{#linkTo 'HMListTags.history' classNames="btn"}}<i class="icon-bar-chart"></i> History{{/linkTo}}
				</div>
			</center>
		</div>
	</div>
	<div class="span2">
		{{#linkTo 'HMListTags.bigIcons' classNames="btn btn-block"}}<i class="icon-plus"></i><br />New How Many{{/linkTo}}
	</div>
	<div class="span5" style="background-color: #eee">
		<div class="row">
			<div class="span5 text-center"><span><i class="icon-filter"></i> Filters</span></div>
		</div>
		<div class="row">
			<center>
				<div class="span5 btn-group btn-toolbar-filters">
					<div class="input-prepend inline" style="margin-bottom:0px;">
						<span class="add-on"><i class="icon-search"></i></span>
						<input class="span2" type="text" placeholder="Search" style="-webkit-border-radius: 0;-moz-border-radius: 0;border-radius: 0;" />
					</div>
					<!-- exclamation on click check-empty -->
					<button class="btn"><i class="icon-check"></i> Pending</button>
					<button class="btn"><i class="icon-share"></i> Shared</button>
					{{#linkTo 'HMList' classNames="btn"}}<i class="icon-unchecked"></i> All{{/linkTo}}
				</div>
			</center>
		</div>
	</div>
</div>